<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="DataBase.Project" table="Project">
        <id name="id" column="IdProject" type="integer">
            <generator class="assigned"></generator>
        </id>
        <property name="descritpion" column="Descritpion" type="string"></property>
        <property name="start" column="Start" type="date"></property>
        <property name="end" column="End" type="date"></property>
        <property name="budget" column="Budget" type="float"></property>
        <property name="cost" column="Cost" type="float"></property>
    </class>
</hibernate-mapping>